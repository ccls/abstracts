<% form_for(@abstract,:url => abstract_cerebrospinal_fluid_path(@abstract)) do |f| %>

<%= f.wrapped_y_n_dk_select(:cerebrospinal_fluid_report_found) %>
<%= f.wrapped_date_text_field(:csf_report_on) %>
<%= f.wrapped_text_field(:csf_white_blood_count) %>
<%= f.wrapped_text_field(:csf_white_blood_count_text) %>
<%= f.wrapped_text_field(:csf_red_blood_count) %>
<%= f.wrapped_text_field(:csf_red_blood_count_text) %>
<%= f.wrapped_y_n_dk_select(:blasts_are_present) %>
<%= f.wrapped_text_field(:number_of_blasts) %>
<%= f.wrapped_y_n_dk_select(:peripheral_blood_in_csf) %>
<%= f.wrapped_text_area(:csf_comment) %>

<p>* Enter Numerical data only</p>


<p>
<%= button_link_to "Cancel", :action => 'show' %>&nbsp;
<%= f.submit 'Save Changes',:name => nil %>
</p>
<% end %>
